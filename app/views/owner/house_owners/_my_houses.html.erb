<section class="pt0 ">
  <div class="owner-my-house">
    <h3 class="uppercase">My Houses</h3>
  <table class="table">
    <thead>
      <tr>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @houses.each do |house| %>
      <tr>
        <td><%= house.id %></td>
        <td><%= house.location %></td>
        <td><%= link_to 'Show', house %></td>
        <td><%= link_to 'Edit', edit_house_path(house) %></td>
        <td><%#= link_to 'Destroy', house, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%#= link_to 'Register another house', new_house_path, class:'btn' %>
  </div>
</section>